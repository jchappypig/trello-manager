<!-- Scaffolding generated by Casein v5.1.1.5 -->

<% cards.select { |label, labelled_cards| label != 'Total' &&  labelled_cards.any? }.each do |label, labelled_cards| %>
    <h3><%= label %></h3>
    <table class="table table-striped table-hover table-condensed">
      <thead>
      <tr>
        <th>Name</th>
        <th>Estimated Size</th>
      </tr>
      </thead>

      <% labelled_cards.each do |card| %>
          <tr>
            <td><%= casein_table_cell_link card.name, card.url, target: '_blank' %></td>
            <td><%= casein_table_cell_link card.estimated_size, card.url, target: '_blank' %></td>
          </tr>
      <% end %>
    </table>
<% end %>